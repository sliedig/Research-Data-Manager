  <UnicastBusConfig ForwardReceivedMessagesTo="audit">
    <MessageEndpointMappings>
      <add Messages="Urdms.ProvisioningService.Messages" Endpoint="urdms.provisioningservice.sharepoint" />
      <add Messages="Urdms.NotificationService.Messages" Endpoint="urdms.notificationservice" />
      <add Messages="Urdms.DocumentBuilderService.Commands" Endpoint="urdms.documentbuilderservice" />
    </MessageEndpointMappings>
  </UnicastBusConfig>

</configuration>